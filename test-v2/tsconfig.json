{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./agents/*"
      ]
    }
  },
  "include": [
    "agents/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}